<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="DistributedLock.Core" Version="1.0.8" />
      <PackageReference Include="MediatR" Version="12.5.0" IncludeAssets="Compile"/>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.9" IncludeAssets="Compile"/>
      <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="9.0.9" IncludeAssets="Compile"/>
      <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="9.0.9" IncludeAssets="Compile"/>
      <PackageReference Include="Microsoft.Extensions.Options.DataAnnotations" Version="9.0.9" IncludeAssets="Compile"/>
    </ItemGroup>

    <PropertyGroup>
        <PackageId>DotNetCore.SharpStreamer</PackageId>
        <Version>1.1.2</Version>
        <Authors>SandrikaMgeli</Authors>
        <RepositoryUrl>https://github.com/SharpStreamer/SharpStreamer</RepositoryUrl>
    </PropertyGroup>
</Project>
